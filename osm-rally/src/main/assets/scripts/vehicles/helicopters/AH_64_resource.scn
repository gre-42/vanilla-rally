macro_begin load_AH_64;
    obj_resource
        name=AH_64-DECIMATE
        filename=models/sketchfab.com/AH-64-apache/AH_64-DECIMATE.obj
        position=0 0 -0.3
        rotation=-5 180 0
        scale=1 1 1
        center_distances=DIST_MIN DIST_MAX
        blend_mode=continuous
        alpha_distances=0 0 INFINITY INFINITY
        cull_faces_alpha=0
        occluded_pass=lightmap_black_global_static
        occluder_pass=OCCLUDER_PASS
        aggregate_mode=none
        transformation_mode=all
        triangle_tangent_error_behavior=zero;
    
    obj_resource
        name=blades_AH_64-DECIMATE
        filename=models/sketchfab.com/AH-64-apache/AH_64-DECIMATE.obj
        position=0 0 0
        rotation=0 0 0
        scale=1 1 1
        center_distances=DIST_MIN DIST_MAX
        blend_mode=continuous
        alpha_distances=0 0 INFINITY INFINITY
        occluded_pass=lightmap_black_global_static
        occluder_pass=OCCLUDER_PASS
        aggregate_mode=none
        transformation_mode=all
        triangle_tangent_error_behavior=zero;
    
    obj_resource
        name=tail_rotor_spinning_AH_64-DECIMATE
        filename=models/sketchfab.com/AH-64-apache/tail_rotor_spinning-DECIMATE.obj
        position=0 0 0
        rotation=0 0 0
        scale=1 1 1
        center_distances=DIST_MIN DIST_MAX
        blend_mode=continuous
        alpha_distances=0 0 INFINITY INFINITY
        occluded_pass=lightmap_black_global_static
        occluder_pass=OCCLUDER_PASS
        aggregate_mode=none
        transformation_mode=all
        triangle_tangent_error_behavior=zero;
macro_end;

macro_playback load_AH_64 DIST_MIN:0   DIST_MAX:100  OCCLUDER_PASS:none                           DECIMATE:;
macro_playback load_AH_64 DIST_MIN:100 DIST_MAX:1000 OCCLUDER_PASS:lightmap_black_local_instances DECIMATE:_d5;

