              "CAR_NODE_POSITION": "%CAR_NODE_POSITION",
              "CAR_NODE_ANGLES": "%CAR_NODE_ANGLES",
              "SUFFIX": "%SUFFIX",
              "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
              "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
              "SPAWNER_NAME": "%SPAWNER_NAME",
