{
  "declare_macro": "setup_teams",
  "content": [
    {
      "call": "set_way_points",
      "arguments": {
        "location": "%osm_location",
        "resource": "%TEAMS_WAY_POINTS_RESOURCE"
      }
    },
    {
      "call": "set_spawn_points",
      "arguments": {
        "location": "%osm_location",
        "resource": "%TEAMS_SPAWN_POINTS_RESOURCE"
      }
    },
    {
      "call": "load_players",
      "arguments": {
        "json": "%PLAYERS_JSON"
      }
    },
    {
      "call": "set_objective",
      "arguments": {
        "objective": "%OBJECTIVE"
      }
    }
  ]
}
