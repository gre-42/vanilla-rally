      {
        "call": "player_create",
        "literals": {
          "name": "%PLAYER_NAME",
          "team": "red",
          "game_mode": "bystander",
          "unstuck_mode": "UNSTUCK_MODE",
          "driving_mode": "pedestrian",
          "driving_direction": "right"
        }
      },
      {
        "call": "set_can_drive",
        "literals": {
