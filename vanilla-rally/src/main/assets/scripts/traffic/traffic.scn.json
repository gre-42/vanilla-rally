[
  {
    "declare_macro": "traffic.create_avatar_npc_player",
    "content": [
      {
        "call": "spawner_create",
        "arguments": {
          "name": "%SPAWNER_NAME",
          "team": "red"
        }
      },
      {
        "call": "player_create",
        "arguments": {
          "name": "%SPAWNER_NAME",
          "team": "red",
          "game_mode": "bystander",
          "unstuck_mode": "%UNSTUCK_MODE",
          "behavior": "pedestrian",
          "driving_direction": "right"
        }
      },
      {
        "call": "set_can_drive",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "source": "ai",
          "value": true
        }
      },
      { "call": "spawner_set_player", "arguments": { "name": "%SPAWNER_NAME", "role": "driver" } }
    ]
  },
  {
    "declare_macro": "traffic.create_car_npc_player",
    "content": [
      {
        "call": "spawner_create",
        "arguments": {
          "name": "%SPAWNER_NAME",
          "team": "red"
        }
      },
      {
        "call": "player_create",
        "arguments": {
          "name": "%SPAWNER_NAME",
          "team": "red",
          "game_mode": "bystander",
          "unstuck_mode": "%UNSTUCK_MODE",
          "behavior": "%behavior",
          "driving_direction": "right"
        }
      },
      { "call": "spawner_set_player", "arguments": { "name": "%SPAWNER_NAME", "role": "driver" } },
      {
        "call": "set_can_drive",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "source": "ai",
          "value": true
        }
      }
    ]
  },
  {
    "declare_macro": "traffic.create_avatar",
    "content": [
      {
        "playback": "create_avatar",
        "arguments": {
          "HUMAN_NODE_POSITION": "%HUMAN_NODE_POSITION",
          "HUMAN_NODE_ANGLE_Y": "%HUMAN_NODE_ANGLE_Y",
          "VELOCITY": "%VELOCITY",
          "ANGULAR_VELOCITY": "%ANGULAR_VELOCITY",
          "HUMAN_NAME": "%HUMAN_NAME",
          "DECIMATE": "%DECIMATE",
          "SUFFIX": "%SUFFIX",
          "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
          "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
          "IF_WITH_GUN": false,
          "SPAWNER_NAME": "%SPAWNER_NAME",
          "IF_SHADOW": false,
          "IF_HUMAN_STYLE": true,
          "COLOR": "%COLOR",
          "IF_SPAWNER": true,
          "IF_PLAYER": true,
          "VELOCITY_ERROR_STD": 0.3,
          "YAW_ERROR_STD": 0.02,
          "PITCH_ERROR_STD": 0.02,
          "ERROR_ALPHA": 0.002,
          "RESPAWN_COOLDOWN_TIME": "%RESPAWN_COOLDOWN_TIME"
        }
      },
      {
        "call": "create_externals",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "mode": "npc"
        }
      },
      {
        "call": "create_internals",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "role": "%role"
        }
      }
    ]
  },
  {
    "declare_macro": "traffic.create_car",
    "content": [
      {
        "playback": "create_car",
        "arguments": {
          "CAR_NODE_POSITION": "%CAR_NODE_POSITION",
          "CAR_NODE_ANGLES": "%CAR_NODE_ANGLES",
          "VELOCITY": "%VELOCITY",
          "ANGULAR_VELOCITY": "%ANGULAR_VELOCITY",
          "CAR_NAME": "%CAR_NAME",
          "DECIMATE": "%DECIMATE",
          "SUFFIX": "%SUFFIX",
          "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
          "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
          "HAND_BRAKE_PULLED": "%HAND_BRAKE_PULLED",
          "IF_PLAYBACK_TRACK": false,
          "IF_PLAYBACK_WINNER": false,
          "IF_SPAWNER": true,
          "IF_PLAYER": true,
          "SPAWNER_NAME": "%SPAWNER_NAME",
          "IF_SHADOW": false,
          "IF_DAMAGEABLE": false,
          "IF_CAR_BODY_RENDERABLE_STYLE": true,
          "COLOR": "%COLOR",
          "VELOCITY_ERROR_STD": null,
          "YAW_ERROR_STD": null,
          "PITCH_ERROR_STD": null,
          "ERROR_ALPHA": null,
          "RESPAWN_COOLDOWN_TIME": "%RESPAWN_COOLDOWN_TIME",
          "MUTE": false
        }
      },
      {
        "call": "create_externals",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "mode": "npc"
        }
      },
      {
        "call": "create_internals",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "role": "%role"
        }
      }
    ]
  },
  {
    "declare_macro": "traffic.create_npc_and_human",
    "content": [
      {
        "playback": "traffic.create_avatar_npc_player",
        "arguments": {
          "SPAWNER_NAME": "%SPAWNER_NAME",
          "UNSTUCK_MODE": "delete"
        }
      },
      {
        "call": "set_pathfinding_way_points",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "node": "osm_map_node",
          "resource": "osm_map"
        }
      },
      {
        "call": "set_preferred_car_spawner",
        "arguments": {
          "spawner": "%SPAWNER_NAME",
          "macro": {
            "playback": "traffic.create_avatar",
            "arguments": {
              "HUMAN_NODE_POSITION": "%HUMAN_NODE_POSITION",
              "HUMAN_NODE_ANGLE_Y": "%HUMAN_NODE_ANGLE_Y",
              "VELOCITY": "%VELOCITY",
              "ANGULAR_VELOCITY": "%ANGULAR_VELOCITY",
              "SUFFIX": "%SUFFIX",
              "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
              "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
              "SPAWNER_NAME": "%SPAWNER_NAME",
              "role": "%role",

              "HUMAN_NAME":"%HUMAN_NAME",
              "COLOR": "%COLOR",
              "DECIMATE":"_d2",
              "RESPAWN_COOLDOWN_TIME": 0
            }
          }
        },
        "let": {
          "HUMAN_NAME": "%HUMAN_NAME",
          "COLOR": "%COLOR"
        }
      }
    ]
  },
  {
    "declare_macro": "traffic.create_npc_and_parking_car",
    "content": [
      {
        "playback": "traffic.create_car_npc_player",
        "arguments": {
          "SPAWNER_NAME": "%SPAWNER_NAME",
          "UNSTUCK_MODE": "off",
          "behavior": "none"
        }
      },
      {
        "call": "set_preferred_car_spawner",
        "arguments": {
          "spawner": "%SPAWNER_NAME",
          "macro": {
            "playback": "traffic.create_car",
            "arguments": {
              "CAR_NODE_POSITION": "%CAR_NODE_POSITION",
              "CAR_NODE_ANGLES": "%CAR_NODE_ANGLES",
              "VELOCITY": "%VELOCITY",
              "ANGULAR_VELOCITY": "%ANGULAR_VELOCITY",
              "SUFFIX": "%SUFFIX",
              "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
              "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
              "SPAWNER_NAME": "%SPAWNER_NAME",
              "role": "%role",

              "CAR_NAME": "%CAR_NAME",
              "COLOR": "%COLOR",
              "DECIMATE": "",
              "HAND_BRAKE_PULLED": true,
              "MUTE": false,
              "RESPAWN_COOLDOWN_TIME": 0
            }
          }
        },
        "let": {
          "CAR_NAME": "%CAR_NAME",
          "COLOR": "%COLOR"
        }
      }
    ]
  },
  {
    "declare_macro": "traffic.create_npc_and_driving_car",
    "content": [
      {
        "playback": "traffic.create_car_npc_player",
        "arguments": {
          "SPAWNER_NAME": "%SPAWNER_NAME",
          "UNSTUCK_MODE": "delete",
          "behavior": "city"
        }
      },
      {
        "call": "set_pathfinding_way_points",
        "arguments": {
          "player": "%SPAWNER_NAME",
          "node": "osm_map_node",
          "resource": "osm_map"
        }
      },
      {
        "call": "set_preferred_car_spawner",
        "arguments": {
          "spawner": "%SPAWNER_NAME",
          "macro": {
            "playback": "traffic.create_car",
            "arguments": {
              "CAR_NODE_POSITION": "%CAR_NODE_POSITION",
              "CAR_NODE_ANGLES": "%CAR_NODE_ANGLES",
              "VELOCITY": "%VELOCITY",
              "ANGULAR_VELOCITY": "%ANGULAR_VELOCITY",
              "SUFFIX": "%SUFFIX",
              "IF_WITH_GRAPHICS": "%IF_WITH_GRAPHICS",
              "IF_WITH_PHYSICS": "%IF_WITH_PHYSICS",
              "SPAWNER_NAME": "%SPAWNER_NAME",
              "role": "%role",

              "CAR_NAME": "%CAR_NAME",
              "COLOR": "%COLOR",
              "DECIMATE": "",
              "HAND_BRAKE_PULLED": false,
              "RESPAWN_COOLDOWN_TIME": 0
            }
          }
        },
        "let": {
          "CAR_NAME": "%CAR_NAME",
          "COLOR": "%COLOR"
        }
      }
    ]
  },
  { "include": "traffic_with_pedestrians.scn.json" },
  { "include": "traffic_wo_pedestrians.scn.json" }
]
