        "IF_PLAYBACK_WINNER": false,
        "PLAYBACK_SPEED": "%PLAYBACK_SPEED",
        "PLAYBACK_FILENAME": "%PLAYBACK_FILENAME",
        "PSUFFIX": "%PSUFFIX",
        "IF_SPAWNER": false,
        "IF_PLAYER": false,
        "IF_SHADOW": true,
        "IF_DAMAGEABLE": false,
