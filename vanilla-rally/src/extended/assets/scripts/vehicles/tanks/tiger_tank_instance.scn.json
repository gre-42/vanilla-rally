          "ammo_type": "tiger_tank_ammo",
          "bullet_type": "tiger_tank_bullet",
          "punch_angle_idle_std": 0.01,
          "punch_angle_shoot_std": 0.1
        }
      },
      {
        "comment": {
          "call": "visual_node_status_3rd",
          "arguments": {
            "node": "car_node$SUFFIX",
            "format": 264,
            "ttf_file": "fonts/RobotoMono-Bold.ttf",
            "offset": [0, 0.3],
            "font_height": 16,
            "line_distance": 16
          }
        }
      },
      {
        "call": "create_engine",
        "arguments": {
          "rigid_body": "car_node$SUFFIX",
          "name": "main",
          "angular_vels": [-2500, -2000, -800, 0],
          "powers": ["nan", 3.6e8, 3.6e8, 0],
          "gear_ratios": [10, 20, 40, -40, -20, -10],
          "w_clutch": 800,
          "max_dw": 2000,
          "hand_brake_pulled": "%HAND_BRAKE_PULLED",
          "audio": {
            "name": "car0",
            "p_idle": 1e7,
            "p_reference": 3.6e8,
            "mute": "%MUTE"
          }
        }
      },
      {
        "call": "create_delta_engine",
        "arguments": { "rigid_body": "car_node$SUFFIX", "name": "left" }
      },
      {
        "call": "create_delta_engine",
        "arguments": { "rigid_body": "car_node$SUFFIX", "name": "right" }
      },
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_left_0_node$SUFFIX",  "vehicle_mount_0": [-1.8, -0.0, -2.4], "vehicle_mount_1": [-1.8, 0.5, -2.4], "radius": 0.25, "engine": "main", "delta_engine": "left",  "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 0}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_right_0_node$SUFFIX", "vehicle_mount_0": [ 1.8, -0.0, -2.4], "vehicle_mount_1": [ 1.8, 0.5, -2.4], "radius": 0.25, "engine": "main", "delta_engine": "right", "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 1}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_left_1_node$SUFFIX",  "vehicle_mount_0": [-1.8, -0.8, -0.8], "vehicle_mount_1": [-1.8, 0.5, -0.8], "radius": 0.25, "engine": "main", "delta_engine": "left",  "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 2}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_right_1_node$SUFFIX", "vehicle_mount_0": [ 1.8, -0.8, -0.8], "vehicle_mount_1": [ 1.8, 0.5, -0.8], "radius": 0.25, "engine": "main", "delta_engine": "right", "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 3}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_left_2_node$SUFFIX",  "vehicle_mount_0": [-1.8, -0.8,  0.8], "vehicle_mount_1": [-1.8, 0.5,  0.8], "radius": 0.25, "engine": "main", "delta_engine": "left",  "brake_force": 2.5e5, "brake_torque": 400, "Ks": 3.00e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 4}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_right_2_node$SUFFIX", "vehicle_mount_0": [ 1.8, -0.8,  0.8], "vehicle_mount_1": [ 1.8, 0.5,  0.8], "radius": 0.25, "engine": "main", "delta_engine": "right", "brake_force": 2.5e5, "brake_torque": 400, "Ks": 3.00e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 5}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_left_3_node$SUFFIX",  "vehicle_mount_0": [-1.8, -0.8,  2.4], "vehicle_mount_1": [-1.8, 0.5,  2.4], "radius": 0.25, "engine": "main", "delta_engine": "left",  "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 6}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_right_3_node$SUFFIX", "vehicle_mount_0": [ 1.8, -0.8,  2.4], "vehicle_mount_1": [ 1.8, 0.5,  2.4], "radius": 0.25, "engine": "main", "delta_engine": "right", "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 7}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_left_4_node$SUFFIX",  "vehicle_mount_0": [-1.8, -0.0,  4  ], "vehicle_mount_1": [-1.8, 0.5,  4  ], "radius": 0.25, "engine": "main", "delta_engine": "left",  "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 8}},
      {"call": "wheel", "arguments": {"vehicle": "car_node$SUFFIX", "wheel": "wheel_right_4_node$SUFFIX", "vehicle_mount_0": [ 1.8, -0.0,  4  ], "vehicle_mount_1": [ 1.8, 0.5,  4  ], "radius": 0.25, "engine": "main", "delta_engine": "right", "brake_force": 2.5e5, "brake_torque": 400, "Ks": 1.67e5, "Ka": 6.7e3, "musF": [6.7e4, 1.33e5], "musC": [1.1, 0.97], "tire_id": 9}},
      {"call": "set_rigid_body_door_distance", "arguments": {"node": "car_node$SUFFIX", "door_distance": 3}},
      {
        "call": "create_tank_controller",
